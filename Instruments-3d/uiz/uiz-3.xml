  <interpolation>
    <entry><ind>	-50.0</ind>	<dep>	-49.0</dep></entry>
    <entry><ind>	150.0</ind>	<dep>	47.0</dep></entry>
  </interpolation>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.02235</y-m>
    <z-m>-0.02288</z-m>
  </center>
  <axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>



