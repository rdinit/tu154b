<!--Needles-->
<animation>
    <type>rotate</type>
    <object-name>needle_1</object-name>
    <!--<property>tu154/fp</property>-->
    <!--Fuel pressure Kg/sm-->
    <interpolation>
      <entry><ind>	0.0</ind>	<dep>	-49.0</dep></entry>
      <entry><ind>	100.0</ind>	<dep>	48.0</dep></entry>
    </interpolation>
    <axis>
      <x>-1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>needle_2</object-name>
  <property alias="../../params/pressure"/>
  <!--Oil pressure Kg/sm-->
  <!--Kg/sm-->
<!--  <interpolation>
    <entry><ind>	0.0</ind>	<dep>	-50.0</dep></entry>
    <entry><ind>	8.0</ind>	<dep>	51.0</dep></entry>
  </interpolation>-->
  <!--Psi-->
  <interpolation>
    <entry><ind>	0.0</ind>	<dep>	-50.0</dep></entry>
    <entry><ind>	113.68</ind>	<dep>	51.0</dep></entry>
  </interpolation>
  <center>
    <x-m>0.0</x-m>
    <y-m>-0.02276</y-m>
    <z-m>-0.02298</z-m>
  </center>
  <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>
