<animation>
  <type>rotate</type>
  <object-name>handle_1</object-name>
  <property>tu154/instrumentation/rsbn/handle-1</property>
  <interpolation>
    <entry><ind>	0.0</ind>	<dep>	30.0</dep></entry>
    <entry><ind>	4.0</ind>	<dep>	150.0</dep></entry>
  </interpolation>
  <center>
    <x-m>0.0</x-m>
    <y-m>-0.04229</y-m>
    <z-m>-0.01822</z-m>
  </center>
  <axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>

